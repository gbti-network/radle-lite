msgstr "{post_title} - 게시물 제목"

msgid "{post_excerpt} - Post Excerpt"
msgstr "{post_excerpt} - 게시물 발췌"

msgid "{post_permalink} - Post Permalink"
msgstr "{post_permalink} - 포스트 고유 주소"

